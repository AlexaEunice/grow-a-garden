{
  "version": 2,
  "builds": [
    {
      "src": "Funcs/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/restock-time", "dest": "/Funcs/GetRestockTime.js" },
    { "src": "/stock", "dest": "/Funcs/GetStock.js" },
    { "src": "/item-info", "dest": "/Funcs/GrabItemInfo.js" },
    { "src": "/weather", "dest": "/Funcs/GrabWeather.js" }
  ],
  "includeFiles": ["Funcs/Database.json"]
}
